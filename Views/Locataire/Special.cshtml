</head>
<body>
    @{
        Layout = "~/Views/Shared/_LayoutLocat.cshtml";
    }
    @model IEnumerable<LoyerVoiture.Models.Voiture>

    @{
        ViewBag.Title = "Special";
    }

    <div class="services" id="service">
        <div class="container">
            <div class="wthree_title_agile">
                <h3> Today <span>SPECIAL</span>  DEALS </h3>
            </div>
            <p class="sub_para">It’s time to drive</p>
            <div class="inner_w3l_agile_grids">
                @foreach (var item in Model)
                {
                    <div class="col-md-4 agileits_service_grid_btm_left">
                        <div class="agileits_service_grid_btm_left1">
                            <div class="agileits_service_grid_btm_left2">
                                <h5>@item.nom</h5>
                                <p>Price<span>:</span>@item.prix Dhs </p>

                            </div>
                            <img src="~/images/@item.image" alt=" " class="img-responsive">
                        </div>
                        @Html.ActionLink(" More details ", "DetailsVoitures", new { id = item.id }, new { @class = "btn btn-primary btn-lg btn-block buy-now" })
                        <br />
                    </div>}
                <div class="clearfix"> </div>
            </div>
        </div>
    </div>

</body>
